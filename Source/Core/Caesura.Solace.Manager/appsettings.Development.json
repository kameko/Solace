{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Storage": {
    "Log": {
      "Path": "./system_log.db",
      "ConnectionString": "Data Source={Path}"
    }
  },
  "Networking":
  {
    "GetLimit": 100
  },
  "Service": {
    "Core.Database": {
      "Connection": "https://localhost:4989"
    }
  }
}
