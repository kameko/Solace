{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Networking":
  {
    "GetLimit": 100
  },
  "Process": {
    "AllowShutdown": true,
    "ShutdownDelayMs": 3000
  },
  "Services": {
    "ReconnectDelayMs": 5000,
    "Items": {
      "Core.Database": {
        "Connection": "https://localhost:4989/",
        "TimeoutMs": 3000
      }
    }
  },
  "Storage": {
    "Log": {
      "Path": "./system_log.db",
      "ConnectionString": "Data Source={Path}"
    }
  }
}
